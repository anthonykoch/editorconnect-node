{
  "private": true,
  "name": "editorconnect-node",
  "version": "1.0.0",
  "description": "Websocket call/reply messaging",
  "@std/esm": "cjs",
  "main": "./lib/index.js",
  "scripts": {
    "test": "ava test/*.test.js",
    "test:watch": "yarn run test --watch",
    "cov": "nyc --reporter=lcov --require @std/esm yarn run test",
    "report": "nyc --require @std/esm yarn run test"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "cuid": "2.1.0",
    "eventemitter2": "5.0.1",
    "lodash": "4.17.4",
    "sinon": "4.3.0",
    "ws": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/plugin-proposal-optional-chaining": "7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "ava": "0.25.0",
    "babel-eslint": "8.0.2",
    "eslint": "4.11.0",
    "nyc": "11.4.1",
    "yargs": "8.0.2"
  },
  "author": "Anthony Koch",
  "license": "MIT"
}
